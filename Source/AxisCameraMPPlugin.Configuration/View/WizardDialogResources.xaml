<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:local="clr-namespace:AxisCameraMPPlugin.Configuration.View"
	xmlns:viewmodel="clr-namespace:AxisCameraMPPlugin.Configuration.ViewModel">
	
	<!-- Merged resources -->
	<ResourceDictionary.MergedDictionaries>
		<ResourceDictionary Source="/AxisCameraMPPlugin.Configuration;Component/VisualStyle/Styles.xaml"/>
	</ResourceDictionary.MergedDictionaries>
	
	<!-- Data template for first wizard page -->
	<DataTemplate DataType="{x:Type viewmodel:WizardPageOneViewModel}">
		<local:WizardPageOne/>
	</DataTemplate>

	<!-- Data template for second wizard page -->
	<DataTemplate DataType="{x:Type viewmodel:WizardPageTwoViewModel}">
		<local:WizardPageTwo/>
	</DataTemplate>

</ResourceDictionary>