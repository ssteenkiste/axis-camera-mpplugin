<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:local="clr-namespace:AxisCameras.Configuration.View"
	xmlns:viewmodel="clr-namespace:AxisCameras.Configuration.ViewModel">
	
	<!-- Merged resources -->
	<ResourceDictionary.MergedDictionaries>
		<ResourceDictionary Source="/AxisCameras.Configuration;Component/VisualStyle/Styles.xaml"/>
	</ResourceDictionary.MergedDictionaries>
	
	<!-- Data template for first wizard page -->
	<DataTemplate DataType="{x:Type viewmodel:WizardPageOneViewModel}">
		<local:WizardPageOne/>
	</DataTemplate>

	<!-- Data template for second wizard page -->
	<DataTemplate DataType="{x:Type viewmodel:WizardPageThreeViewModel}">
		<local:WizardPageThree/>
	</DataTemplate>

</ResourceDictionary>